# Here also go all build targets specific to this very newgrf

GENERATE_NML := road-hog-nml
GENERATE_LNG := road-hog-nml
GENERATE_DOC := road-hog-nml

GENERATE=src/build_road_hog.py

road-hog-nml:
	$(_E) [GENERATE]
	$(_V) PYTHONPATH=${PYTHONPATH2} ${PYTHON2} src/build_road_hog.py '${REPO_TITLE}' '${REPO_REVISION}' '${COMPILE_FASTER}' '${NO_MP}'

clean::
	$(_V)-rm road-hog.nml
	$(_V)-rm -r docs
	$(_V)-rm -r chameleon_cache
	$(_V)-rm -r generated
	$(_V)-rm src/*.pyc
	$(_V)-rm src/*/*.pyc

# Here also go all build targets specific to this very newgrf

GENERATE_NML := road-hog-nml
GENERATE_LNG := road-hog-nml
GENERATE_DOC := road-hog-nml

GENERATE=src/build_road_hog.py

road-hog-nml:
	$(_E) [GENERATE]
	$(_V) python src/build_road_hog.py '${REPO_TITLE}' '${REPO_REVISION}'

clean::
	$(_V)-rm iron-horse.nml
	$(_V)-rm -r docs
	$(_V)-rm -r chameleon_cache
	$(_V)-rm -r generated
